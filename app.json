{
  "expo": {
    "name": "Capitec",
    "slug": "capitecbank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "capitecbank",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": false,
      "package": "com.mickytroxxy.capitecbank"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0b9bab75-a9f2-43c6-b149-32b2c6d1e702"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "empiredigitalsmain",
    "updates": {
      "url": "https://u.expo.dev/0b9bab75-a9f2-43c6-b149-32b2c6d1e702"
    }
  }
}
